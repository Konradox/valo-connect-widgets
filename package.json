{
  "name": "valo-connect-widgets",
  "version": "0.0.1",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "start": "gulp serve --nobrowser",
    "build": "gulp bundle",
    "clean": "gulp clean",
    "test": "gulp test"
  },
  "dependencies": {
    "@fluentui/react-northstar": "^0.53.0",
    "@microsoft/decorators": "1.12.0",
    "@microsoft/sp-application-base": "1.12.0",
    "@microsoft/sp-core-library": "1.12.0",
    "@microsoft/sp-dialog": "1.12.0",
    "@valo/extensibility": "^1.8.0-b973bb592e02a7a1d9e85a40f68cee3b365f61b1"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.7": "0.2.3",
    "@microsoft/sp-build-web": "1.12.0",
    "@microsoft/sp-module-interfaces": "1.12.0",
    "@microsoft/sp-tslint-rules": "1.12.0",
    "@microsoft/sp-webpart-workbench": "1.12.0",
    "@types/chai": "^4.2.15",
    "@types/jest": "^26.0.21",
    "@types/webpack-env": "1.13.1",
    "ajv": "~5.2.2",
    "chai": "^4.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gulp": "~4.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(ts?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "@ms/sp-telemetry": "identity-obj-proxy",
      "@microsoft/sp-webpart-base": "identity-obj-proxy",
      "@microsoft/sp-application-base": "identity-obj-proxy",
      "^<loc-file-name>Strings": "identity-obj-proxy",
      "^resx-strings/en-us.json": "<rootDir>/node_modules/@microsoft/sp-core-library/lib/resx-strings/en-us.json",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
